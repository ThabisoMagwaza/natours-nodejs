{
  "name": "natours",
  "version": "1.0.0",
  "description": "complete node bootcamp project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "debug": "ndb server.js",
    "start:prod": "set NODE_ENV=production&& npm start",
    "populateDevDB": "node ./scripts/tour-data.js --populate",
    "clearDevDB": "node ./scripts/tour-data.js --delete",
    "resetDevDB": "node ./scripts/tour-data.js --reset",
    "watch:js": "parcel watch .\\public\\js\\index.js --out-dir .\\public\\js --out-file bundle.js",
    "build:js": "parcel build .\\public\\js\\index.js --out-dir .\\public\\js --out-file bundle.js"
  },
  "author": "Thabiso Magwaza",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.1",
    "express-rate-limit": "^5.2.3",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "pug": "^3.0.0",
    "sharp": "^0.27.0",
    "slugify": "^1.4.6",
    "validator": "^13.1.17",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "eslint": "^7.18.0",
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": [
    "since 2017-06"
  ]
